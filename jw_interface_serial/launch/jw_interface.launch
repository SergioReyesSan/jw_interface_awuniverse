<launch>
  <arg name="serial_port" default="/dev/ttyUSB0"/>
  <arg name="vehicle_cmd_timeout" default="0.2"/>

  <node pkg="jw_interface_serial" name="jw_interface" type="jw_interface_serial_node" output="screen">
    <param name="serial_port" value="$(arg serial_port)" />
    <param name="vehicle_cmd_timeout" value="$(arg vehicle_cmd_timeout)" />
  </node>

</launch>
